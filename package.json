{
  "name": "contractor-management-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:migrate": "psql $DATABASE_URL -f scripts/fixed-migration.sql",
    "db:seed": "psql $DATABASE_URL -f scripts/seed-data.sql",
    "db:reset": "psql $DATABASE_URL -c 'DROP SCHEMA public CASCADE; CREATE SCHEMA public;' && npm run db:migrate && npm run db:seed",
    "db:backup": "./scripts/backup-database.sh",
    "db:restore": "./scripts/restore-database.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
